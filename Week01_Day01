// Q-1
#include <iostream>
#include<string>
using namespace std;
int main() {
    string n;
    string m;
    cin>>n>>m;
    cout<<n+m;
    return 0;
}

// Q2
#include <iostream>
using namespace std;
int main() {
    int a[5];
    for(int i=0;i<5;i++){
        cin>>a[i];
    }
    float sum;
    for(int i=0;i<5;i++){
        sum+=a[i];
    }
    float ans=sum/5;
    cout<<ans;
    return 0;
}

// Q3
#include <iostream>
using namespace std;
int main() {
    int n;
    cin>>n;
    cout<<n*n;
    return 0;
}
